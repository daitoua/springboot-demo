<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.daitou.springbootdemo.api.dao.QueryUserMapper">

    <select id="queryUser" resultType="com.daitou.springbootdemo.api.model.User">
        select
            id,username,password
        from t_user
